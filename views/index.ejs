<% include ./partials/header %>
    <div class="page-header" data-parallax="true" style="background-image: url(http://34.192.214.125:80/home3.png);">
                <div class="container">
                  <center>
                    <h1 class="title" style="color:black" >Beacon Ticket Automate</h1>
                    <strong><h2 style="color:black">no more rush, no more asking for change</h2></strong>
                  </center>

                </div>

    </div>

    <div class="main">
        <div class="section section-tabs">
            <div class="container">
                <div id="nav-tabs">
                  <center>
                    <h1 class='title'>Navigation Tabs</h3>

                  </center>
                    <div class="row">
                        <div class="col-md-8 col-md-offset-2">
                            <!-- Tabs with icons on Card -->
                            <div class="card card-nav-tabs">
                                <div class="header header-primary">
                                    <div class="nav-tabs-navigation">
                                        <div class="nav-tabs-wrapper">
                                            <ul class="nav nav-tabs" data-tabs="tabs" style="display: flex; justify-content: center;">
                                                <li class="active">
                                                    <a href="#login" data-toggle="tab" aria-expanded="true">
                                                        <i class="material-icons">face</i>
                                                        Login
                                                    <div class="ripple-container"></div></a>
                                                </li>
                                                <li class="">
                                                    <a href="#signup" data-toggle="tab" aria-expanded="false">
                                                        <i class="material-icons">chat</i>
